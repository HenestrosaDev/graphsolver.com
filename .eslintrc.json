{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", 2],
		"no-trailing-spaces": "error",
		"no-multiple-empty-lines": ["error", { "max": 1 }],
		"eol-last": "error"
	}
}